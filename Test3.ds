  <SQL>SELECT id, name FROM pz_dim_store a 
 WHERE :p1 IS NULL OR id = ANY(string_to_array(:p1,',')::int[])
UNION
SELECT NULL AS id,'(All)' AS name
 WHERE :p2 = 1
 ORDER BY name;

</SQL>
